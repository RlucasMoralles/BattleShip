                "in": "codCliente"
              }
            },
            {
              "for": {
                "in": "nomeCliente"
              }
            },
            {
              "for": {
                "in": "codProduto"
              }
            },
            {
              "for": {
                "in": "nomeProduto"
              }
            },
            {
              "for": {
                "in": "qtVendida"
              }
            }
          ]
        }
      }
    }
  ],
  "objects": {
    "general": {
      "displayName": "Geral",
      "properties": {
        "buttonText": {
          "displayName": "Texto do Botão",
          "type": {
            "text": true
          }
        },
        "buttonColor": {
          "displayName": "Cor do Botão",
